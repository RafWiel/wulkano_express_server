{
  "name": "wulkano_express_server",
  "version": "1.0.0",
  "description": "Wulkano workshop service",
  "main": "index.js",
  "bin": "src/app.js",
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "signatures/**/*"
    ],
    "outputPath": "dist",
    "scripts": "src/**/*.js",
    "targets": [
      "node14-windows-x64"
    ]
  },
  "scripts": {
    "start": "node src/app.js",
    "local": "nodemon --watch src src/app.js --exec \"npm run lint && node\"",
    "lint": "eslint **/*.js",
    "build": "pkg package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafWiel/wulkano_express_server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RafWiel/wulkano_express_server/issues"
  },
  "homepage": "https://github.com/RafWiel/wulkano_express_server#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.17.3",
    "helmet": "^6.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "npm": "^8.6.0",
    "sequelize": "^6.18.0"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "nodemon": "^2.0.15"
  }
}
